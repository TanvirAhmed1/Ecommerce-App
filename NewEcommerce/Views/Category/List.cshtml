@using Ecommerce.Models.EntityModels
@model List<Ecommerce.Models.EntityModels.Catagory>


<h1>Category List</h1>
<table class="table table-striped table-hover table-bordered">
    <thead>
        <tr>
            <th>
                Name
            </th>

            <th>Action</th>
        </tr>


    </thead>
    <tbody>

        @foreach (Catagory category in Model)
        {
            <tr>
                <td>@category.Name</td>

                <td>
                    <a asp-action="Edit" asp-route-id="@category.Id" class="btn btn-primary categoryEdit">Edit</a> |
                    <a asp-action="Delete" asp-route-id="@category.Id" class="btn btn-danger categoryEdit">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
